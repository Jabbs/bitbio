
<div class="row">
  <div class="span9">
    <% if @users && !@users.empty? %>
      <% @users.each do |user| %>
        <%= render 'user', user: user %>
      <% end %>
    <% else %>
      <h4><em>None. Please sign up <%= link_to 'here', new_user_path %>.</em></h4>
    <% end %>
  </div>
</div>
<%= will_paginate %>